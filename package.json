{
    "name": "color-palette",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "panda codegen",
        "build": "pnpm dlx concurrently \"pnpm:build:*\"",
        "build:pages": "react-router build",
        "build:storybook": "storybook build",
        "dev": "pnpm dlx concurrently -k -s first \"pnpm dev:pages\" \"pnpm dev:storybook\"",
        "dev:pages": "react-router dev",
        "dev:storybook": "storybook dev -p 6006",
        "serve": "pnpm dlx http-server storybook-static -p 6006 --silent",
        "test": "test-storybook --junit",
        "test:vrt": "playwright test",
        "test:runner": "pnpm build:storybook && pnpm dlx concurrently -k -s first \"pnpm serve\" \"pnpm dlx wait-on tcp:6006 && pnpm test\"",
        "cf-typegen": "wrangler types",
        "deploy": "pnpm run pages:build && wrangler deploy",
        "preview": "pnpm run pages:build && vite preview",
        "check": "biome check ./ --write",
        "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
        "kill": "kill $(lsof -t -i:6006)"
    },
    "dependencies": {
        "@chakra-ui/charts": "^3.17.0",
        "@chakra-ui/react": "^3.17.0",
        "@emotion/react": "^11.14.0",
        "@storybook/addon-themes": "^8.6.12",
        "@types/file-saver": "^2.0.7",
        "apca-w3": "^0.1.9",
        "autoprefixer": "^10.4.21",
        "colorparsley": "^0.1.8",
        "culori": "^4.0.1",
        "file-saver": "^2.0.5",
        "isbot": "^5.1.27",
        "jotai": "^2.12.5",
        "jotai-location": "^0.6.1",
        "jszip": "^3.10.1",
        "lucide-react": "^0.513.0",
        "markdown-to-jsx": "^7.7.6",
        "next-themes": "^0.4.6",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-icons": "^5.5.0",
        "react-router": "^7.5.3",
        "recharts": "^2.15.3",
        "remark-gfm": "^4.0.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@chromatic-com/storybook": "^3.2.6",
        "@cloudflare/vite-plugin": "^1.1.0",
        "@cloudflare/workers-types": "^4.20250505.0",
        "@pandacss/dev": "^0.53.6",
        "@playwright/test": "^1.52.0",
        "@react-router/dev": "^7.5.3",
        "@storybook/addon-a11y": "^8.6.12",
        "@storybook/addon-essentials": "^8.6.12",
        "@storybook/addon-onboarding": "^8.6.12",
        "@storybook/blocks": "^8.6.12",
        "@storybook/experimental-addon-test": "^8.6.12",
        "@storybook/manager-api": "^8.6.12",
        "@storybook/react": "^8.6.12",
        "@storybook/react-vite": "^8.6.12",
        "@storybook/test": "^8.6.12",
        "@storybook/test-runner": "^0.22.0",
        "@storybook/theming": "^8.6.12",
        "@types/culori": "^4.0.0",
        "@types/node": "^22.15.3",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "@vitest/browser": "^3.1.2",
        "@vitest/coverage-v8": "^3.1.2",
        "axe-playwright": "^2.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^7.4.0",
        "cz-emoji": "1.3.2-canary.2",
        "lefthook": "^1.11.12",
        "playwright": "^1.52.0",
